{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "config_deployments",
  "info": {
    "name": "ConfigDeployment"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "deployed",
        "stopped",
        "failed"
      ],
      "required": true
    },
    "game_config": {
      "via": "config_deployments",
      "model": "game-config"
    }
  }
}
