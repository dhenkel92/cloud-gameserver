{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "game_configs",
  "info": {
    "name": "GameConfig",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "game": {
      "model": "game",
      "via": "game_configs"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false,
      "maxLength": 50
    },
    "game_deployments": {
      "collection": "game-deployment",
      "via": "game_config"
    },
    "configuration": {
      "type": "json",
      "required": true
    },
    "game_servers": {
      "collection": "game-server",
      "via": "game_config"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "game_configs"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "STARTING",
        "RUNNING",
        "STOPPING",
        "STOPPED",
        "FAILED"
      ],
      "required": true,
      "default": "STOPPED"
    }
  }
}
