{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "game_configs",
  "info": {
    "name": "GameConfig",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "game": {
      "model": "game",
      "via": "game_configs"
    },
    "s3_base_path": {
      "type": "string",
      "maxLength": 150,
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false,
      "maxLength": 50
    },
    "startup_script": {
      "type": "text",
      "required": true
    },
    "server_config": {
      "type": "text",
      "required": true
    },
    "game_deployments": {
      "via": "game_config",
      "collection": "game-deployment"
    }
  }
}
